<Window x:Class="Salesautopilot_API_Client_App.AddNewAPIContact"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Salesautopilot_API_Client_App"
        mc:Ignorable="d"
        Title="AddNewAPIContact" Height="350" Width="500">
    <StackPanel Margin="15">
        <TextBox x:Name="addAPIContactName"/>
        <TextBox x:Name="addAPIContactKeyUserName"/>
        <TextBox x:Name="addAPIContactKeyPassword"/>
        <Button Content="Save" x:Name="saveButton" Click="saveButton_Click"/>
        <Button Content="Cancel" x:Name="addApiContactPanelCancelButton" Click="addApiContactPanelCancelButton_Click"/>
    </StackPanel>
</Window>
